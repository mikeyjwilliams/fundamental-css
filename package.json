{
  "name": "fundamental-css",
  "version": "0.0.1",
  "description": "sass based utility library",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "npx http-server ./ -p 8080 -a 127.0.0.1",
    "build:scss": "sass sass/base.scss public/build.css",
    "scss:watch": "sass --watch sass/base.scss public/build.css",
    "gulp:stats": "gulp stats",
    "gulp:prod": "gulp production",
    "gulp:dev": "gulp dev",
    "gulp:test": "gulp test",
    "gulp:qa": "gulp qa",
    "production": "cross-env NOD_ENV=production npm run gulp:qa",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeyjwilliams/fundamental-css.git"
  },
  "keywords": [
    "scss",
    "css",
    "utility-library"
  ],
  "author": "Michael Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikeyjwilliams/fundamental-css/issues"
  },
  "homepage": "https://github.com/mikeyjwilliams/fundamental-css#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "csscomb": "^4.3.0",
    "cssnano": "^4.1.10",
    "cssstats": "^3.4.1",
    "gulp": "^4.0.2",
    "gulp-bytediff": "^1.0.0",
    "gulp-crass": "^0.2.2",
    "gulp-csscss": "^0.1.2",
    "gulp-cssnano": "^2.1.3",
    "gulp-csso": "^4.0.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-filter": "^6.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sizereport": "^1.2.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-strip-css-comments": "^2.0.0",
    "gulp-stylestats": "^3.1.0",
    "pixrem": "^5.0.0",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-pxtorem": "^5.1.1",
    "sass": "^1.26.7",
    "tailwindcss": "^1.4.6"
