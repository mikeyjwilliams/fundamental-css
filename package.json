{
  "name": "fundamental-css",
  "version": "0.0.2",
  "description": "sass based utility library",
  "main": "index.js",
  "scripts": {
    "server": "php -S localhost:8080",
    "build:scss": "sass sass/build.scss public/css/build.css",
    "scss:watch": "sass --watch sass/build.scss public/css/build.css",
    "build:tail": "npx tailwind build tail/tail.css -o tail/tailwind.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeyjwilliams/fundamental-css.git"
  },
  "keywords": [
    "scss",
    "css",
    "utility-library"
  ],
  "author": "Michael Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikeyjwilliams/fundamental-css/issues"
  },
  "homepage": "https://github.com/mikeyjwilliams/fundamental-css#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "browser-sync": "^2.26.7",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "csscomb": "^4.3.0",
    "cssnano": "^4.1.10",
    "cssstats": "^3.4.1",
    "grunt": "^1.2.1",
    "grunt-combine-media-queries": "^1.0.20",
    "grunt-postcss": "^0.9.0",
    "grunt-sass": "^3.1.0",
    "grunt-strip-css-comments": "^1.3.0",
    "grunt-stripcomments": "^0.7.2",
    "load-grunt-tasks": "^5.1.0",
    "node-sass": "^4.14.1",
    "pixrem": "^5.0.0",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.0.5",
    "sass": "^1.26.7",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "highlight.js": "^10.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  }
}
